{
    "manifest_version": 3,
    "name": "MPC Extension",
    "version": "0.0.1",
    "description": "MPC Extension",
    "permissions": ["scripting", "activeTab"],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "./assets/img/16.png",
            "24": "./assets/img/24.png",
            "32": "./assets/img/32.png",
            "48": "./assets/img/48.png",
            "128": "./assets/img/128.png"
        }
    }
}
